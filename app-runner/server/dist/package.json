{
  "name": "app-runner",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "sqlite3": "^5.0.0"
  },
  "proxy": "http://localhost:5000/",
  "scripts": {
    "build": "rimraf dist/ && ./node_modules/.bin/babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "server": "env JAWHAR_IP=$(route -n | grep 'UG[ \t]' | awk '{print $2}') babel-watch --watch HurraApp server.js HurraServer.js"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.3.4",
    "babel-watch": "^7.0.0",
    "rimraf": "^2.6.3"
  }
}
